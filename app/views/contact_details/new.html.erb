<section id="contact">
  <%= simple_form_for @contact_details, url: contact_details_path, method: :post do |f| %>
    <%= f.input :business_name %>
    <%= f.input :telephone_number %>
    <%= f.input :email %>
    <%= f.input :contact_time %>
    <%= f.input :notes %>
    <%= f.input :reference %>

    <%= f.submit %>
  <% end %>
</section>
